26
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Example: 5

    for (int i = 0; i < n; i++) {
        char ch = 'a' + i;
        for (int j = 0; j <= i; j++) {
            printf("%c", ch);
        }
        printf("\n");
    }

    return 0;
}
27#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Example input: 5

    // Upper half: A to ABC... up to length n
    for (int i = 1; i <= n; i++) {
        for (char ch = 'A'; ch < 'A' + i; ch++) {
            printf("%c", ch);
        }
        printf("\n");
    }

    // Lower half: from n-1 back to 1
    for (int i = n - 1; i >= 1; i--) {
        for (char ch = 'A'; ch < 'A' + i; ch++) {
            printf("%c", ch);
        }
        printf("\n");
    }

    return 0;
}
28 
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Example: 5

    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= i; j++) {
            printf("%c%c", 'A' + j, 'a' + j);
        }
        printf("\n");
    }

    return 0;
}
29
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Example: 5

    char ch = 'A';

    // Upper part (1 to n)
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%c", ch);
        }
        printf("\n");
        ch++;
    }

    // Lower part (n-1 to 1)
    for (int i = n - 1; i >= 1; i--) {
        for (int j = 1; j <= i; j++) {
            printf("%c", ch);
        }
        printf("\n");
        ch++;
    }

    return 0;
}
30
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Example: 5

    char start = 'A' + n - 1;

    // Upper half (including middle line)
    for (int i = 1; i <= n; i++) {
        for (char ch = start; ch >= start - i + 1; ch--) {
            printf("%c", ch);
        }
        printf("\n");
    }

    // Lower half
    for (int i = n - 1; i >= 1; i--) {
        for (char ch = start; ch >= start - i + 1; ch--) {
            printf("%c", ch);
        }
        printf("\n");
    }

    return 0;
}
