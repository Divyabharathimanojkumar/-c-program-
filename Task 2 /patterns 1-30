20 
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Number of rows

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= 2 * i - 1; j++) {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
21
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Read input number

    for (int i = 1; i <= n; i++) {
        for (char ch = 'A'; ch < 'A' + i; ch++) {
            printf("%c", ch);
        }
        printf("\n");
    }

    return 0;
}
22
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Read input

    for (int i = 0; i < n; i++) {
        for (char ch = 'a' + i; ch >= 'a'; ch--) {
            printf("%c", ch);
        }
        printf("\n");
    }

    return 0;
}
23
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);  // Read input

    for (int i = n; i >= 1; i--) {
        for (char ch = 'A' + i - 1; ch >= 'A'; ch--) {
            printf("%c", ch);
        }
        printf("\n");
    }

    return 0;
}
